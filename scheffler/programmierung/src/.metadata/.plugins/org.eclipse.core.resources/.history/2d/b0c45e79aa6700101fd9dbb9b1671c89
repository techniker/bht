#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int var1;
	var1 = 2;
	int var2;

	  printf ( "Die Ausgabe ist: %i\n", var1);
	  //foo(var1);
	  printf ("foo function: %i\n", foo(var1));
	return EXIT_SUCCESS;
}

void foo(var2) {

	var2 += 6;

	//printf ("%i\n", var2);
	var1 = var2;
	return 0;
}
