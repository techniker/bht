#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(void) {

	time_t zeit;
	  time ( &zeit );
	  printf ( "Die Ausgabe von ctime ist: %s\n", ctime (&zeit));

	return EXIT_SUCCESS;
}
